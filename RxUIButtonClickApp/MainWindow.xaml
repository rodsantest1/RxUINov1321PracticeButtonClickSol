<reactiveui:ReactiveWindow  x:Class="RxUIButtonClickApp.MainWindow"
        x:TypeArguments="vms:AppViewModel"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:RxUIButtonClickApp"
        xmlns:vms="clr-namespace:ClassLibrary1;assembly=ClassLibrary1"
        mc:Ignorable="d"
        xmlns:reactiveui="http://reactiveui.net"
        Title="MainWindow" Height="450" Width="800">
    <StackPanel Orientation="Horizontal">
        <Button Margin="5 0 0 0" Width="125" Height="50" x:Name="TestButton" Content="Test" />
        <Button Margin="5 0 0 0" Width="125" Height="50" x:Name="TestButton2" Content="Test Short" />
        <Button Margin="5 0 0 0" Width="125" Height="50" x:Name="TestButton3" Content="Test Short2" />
    </StackPanel>
</reactiveui:ReactiveWindow>
